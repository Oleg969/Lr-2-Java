package com.company;

public class Student {
    String name;
    String surname;
    int age;
    public static int compareStudentsName( Student a, Student b){
        return a.name.compareTo(b.name);
    }
    public static int compareStudentsSurname( Student a, Student b){
        return a.surname.compareTo(b.surname);
    }
    public static int compareStudentsAge( Student a, Student b){
        return a.getAge() - b.getAge();
    }

    private int getAge() {
        return age;
    }

    public Student(String name, String surname, int age) {
        this.name = name;
        this.surname = surname;
        this.age = age;
    }
}
